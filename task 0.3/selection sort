########### selection sort #####################
#include<stdio.h>
#include<stdlib.h>


int main()
{

int a[100000],i,j,count=1,k,t,min,m;

for(i=0;i<100000;i++)
    {
    a[i]=m%(1000000+(i*i));
    }

for(i=0;i<100000;i++)
    {
    for(j=i+1;j<100000;j++)
        {
        min=9999999999;
        for(t=j;t<100000;t++)
            {
            if(a[t]<min)
                {
                min=a[t];
                k=t;
                }
            }
        if(a[i]>a[k])
            {
            a[i]=a[i]+a[k];
            a[k]=a[i]-a[k];
            a[i]=a[i]-a[k];
            }
            break;
        }
    }

/* print a here*/

}
